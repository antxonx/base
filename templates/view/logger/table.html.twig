<table class="table table-sm w-100 table-hover2 table-bordered2 table-striped2-4">
    {% set sort %}
        <span class="float-right sort-icon"><i class="fas fa-sort"></i></span>
    {% endset %}
    {% include "table/thead.html.twig" with {
        tr : {
            values : {
                id : {
                    value : 'Id' ~ sort,
                    attr: {
                        class: 'cursor-pointer sort-column',
                        column: 'id',
                        style: 'width: 7%;'
                    }
                },
                creation : {
                    value : 'Creación' ~ sort,
                    attr : {
                        class: 'cursor-pointer sort-column',
                        column: 'createdAt',
                        style: 'width: 12%;'
                    }
                },
                method : {
                    value : 'Método',
                    attr: {
                        style: 'width: 6%;'
                    }
                },
                route : {
                    value : 'Ruta',
                    attr : {
                        style: 'width: 25%;'
                    }
                },
                message : {
                    value : 'Mensaje',
                    attr : {
                        style: 'width: 50%;'
                    }
                },
            }
        }
    } %}
    <tbody id="logsView">

    </tbody>
</table>
